{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://temportalflux.github.io/QA-Recorder",
  "scripts": {
    "buildDocs": "node $(npm bin)/react-docgen ../src/components/ --pretty --out ./src/reportMain.json && react-docgen ./src/components/ --pretty --out ./src/reportDocs.json",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages-with-token -d build"
  },
  "dependencies": {
    "keyboard-key": "^1.0.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-static": "^5.9.7",
    "shallowequal": "^1.1.0",
    "semantic-ui-react": "^0.79.1",
    "react-markdown": "^3.4.1"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "gh-pages-with-token": "^1.0.0",
    "install": "^0.12.1",
    "react-docgen": "^2.21.0"
  }
}
